{
  "app": {
    "prefix": "nf",
    "name": "titus-app"
  },
  "dev": {
    "target": "default"
  },
  "accounts": {
    "default": {
      "env": {
        "account": "912147919779",
        "region": "eu-west-2",
        "domainName": "mira-nf.com",
        "webAppUrl": "titus-dev.mira-nf.com",
        "certificateSslName": "titus-dev.mira-nf.com",
        "awsEcrRepositoryName": "mira-nf--titus-backend"
      },
      "profile": "default"
    },
    "cicd": {
      "env": {
        "account": "912147919779",
        "region": "eu-west-2"
      },
      "profile": "default"
    },
    "staging": {
      "env": {
        "account": "912147919779",
        "region": "eu-west-2",
        "domainName": "mira-nf.com",
        "webAppUrl": "titus-stage.mira-nf.com",
        "certificateSslName": "titus-stage.mira-nf.com",
        "awsEcrRepositoryName": "mira-nf--titus-backend"
      },
      "profile": "default"
    },
    "production": {
      "env": {
        "account": "912147919779",
        "region": "eu-west-2",
        "domainName": "mira-nf.com",
        "webAppUrl": "titus.mira-nf.com",
        "certificateSslName": "titus.mira-nf.com",
        "awsEcrRepositoryName": "mira-nf--titus-backend"
      },
      "profile": "default"
    }
  },
  "cicd": {
    "target": "cicd",
    "buildspecFile": "packages/titus-infra-aws-mira/pipeline/buildspec.yaml",
    "permissionsFile": "pipeline/permissions.js",
    "provider": "codecommit",
    "branchName": "master",
    "codeCommitUserPublicKey": "ssh-rsa YOUR_PUBLIC_CODE_COMMIT_KEY",
    "stages": [
      {
        "target": "staging",
        "withDomain": false,
        "requireManualApproval": false,
        "privileged": true
      },
      {
        "target": "production",
        "withDomain": false,
        "requireManualApproval": true,
        "privileged": true
      }
    ]
  }
}
