{
  "app": {
    "prefix": "nf",
    "name": "titus-app"
  },
  "dev": {
    "target": "default"
  },
  "accounts": {
    "default": {
      "env": {
        "account": "123456789",
        "region": "eu-central-1",
        "domainName": "nf-mira.com",
        "webAppUrl": "titus-dev.nf-mira.com",
        "certificateSslName": "*.nf-mira.com",
        "awsEcrRepositoryName": "nf-mira-titus-backend"
      },
      "profile": "default"
    },
    "cicd": {
      "env": {
        "account": "123456789",
        "region": "eu-central-1"
      },
      "profile": "default"
    },
    "staging": {
      "env": {
        "account": "123456789",
        "region": "eu-central-1",
        "domainName": "nf-mira.com",
        "webAppUrl": "titus-stage.nf-mira.com",
        "certificateSslName": "*.nf-mira.com",
        "awsEcrRepositoryName": "nf-mira-titus-backend"
      },
      "profile": "default"
    },
    "production": {
      "env": {
        "account": "123456789",
        "region": "eu-central-1",
        "domainName": "nf-mira.com",
        "webAppUrl": "titus.nf-mira.com",
        "certificateSslName": "*.nf-mira.com",
        "awsEcrRepositoryName": "nf-mira-titus-backend"
      },
      "profile": "default"
    }
  },
  "cicd": {
    "target": "cicd",
    "buildspecFile": "packages/titus-infra-aws-mira/pipeline/buildspec.yaml",
    "permissionsFile": "pipeline/permissions.js",
    "provider": "codecommit",
    "branchName": "master",
    "codeCommitUserPublicKey": "ssh-rsa SET THE PUBLICK KEY HERE",
    "stages": [
      {
        "target": "staging",
        "withDomain": false,
        "requireManualApproval": false,
        "privileged": true
      },
      {
        "target": "production",
        "withDomain": false,
        "requireManualApproval": true,
        "privileged": true
      }
    ]
  }
}
